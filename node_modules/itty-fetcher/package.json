{
  "name": "itty-fetcher",
  "version": "1.0.10",
  "type": "module",
  "description": "Fetch, without the boilerplate (and Typed).",
  "main": "./fetcher.js",
  "module": "./fetcher.mjs",
  "types": "./fetcher.d.ts",
  "exports": {
    ".": {
      "import": "./fetcher.mjs",
      "types": "./fetcher.d.ts",
      "require": "./fetcher.cjs"
    }
  },
  "scripts": {
    "dev": "bun test --watch --coverage",
    "test": "bun test --coverage",
    "lint": "itty lint",
    "build": "itty lint && itty build --snippet=fetcher --hybrid",
    "release": "itty release --prepare --tag --push",
    "release:next": "itty release --prepare --tag --push --type=next"
  },
  "keywords": [
    "fetch",
    "api",
    "composable",
    "tiny",
    "simple",
    "json"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kwhitley/itty-fetcher.git"
  },
  "author": "Kevin R. Whitley <krwhitley@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kwhitley/itty-fetcher/issues"
  },
  "devDependencies": {
    "@types/bun": "^1.2.17",
    "itty-packager": "^1.6.10"
  }
}
